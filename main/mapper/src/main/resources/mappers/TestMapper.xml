<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzd.dao.TestDAO">
    <resultMap id="testMap" type="Test">
        <id column="test" property="test"/>
    </resultMap>
    <select id="findFirstTest" resultMap="testMap">
        select `test`
        from test where test = #{test}
        limit 0, 1
    </select>


</mapper>