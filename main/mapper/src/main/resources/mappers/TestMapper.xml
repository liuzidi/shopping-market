<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzd.dao.TestDAO">

    <select id="findFirstTest" resultType="Test">
        select `test`
        from test where test = #{test}
        limit 0, 1
    </select>


</mapper>